<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struct SYSTEM "lcd-struct.dtd" [
  <!ENTITY % alglen SYSTEM "alglen.dtd"> %alglen;
]>
<struct name="transaction">
  <field name="trx_id" size="&TRX_ID_LEN;"/>
  <field name="type_id" size="&TRX_TYP_LEN;"/>
  <field name="rtn_code" size="&RTN_CODE_LEN;"/>
  <field name="stocker_id" size="&EQPT_ID_LEN;"/>
  <field name="bay_id" size="&BAY_ID_LEN;"/>
  <field name="bcs_node" size="&BCS_NODE_LEN;"/>
  <field name="xfer_node" size="&BCS_NODE_LEN;"/>
  <field name="stocker_dsc" size="&EQPT_DSC_LEN;"/>
  <field name="total_shelf_cnt" size="&ZONE_T_CNT_LEN;"/>
  <field name="threshold" size="&ZONE_CNT_LEN;"/>
  <field name="eqpt_subtyp" size="1"/>
  <field name="bcs_node2" size="&BCS_NODE_LEN;"/>
  <field name="floor_code" size="&FLOOR_CODE_LEN;"/>
  <field name="zone_cnt" size="2"/>
  <struct name="oary1" multiplicity="20">
    <field name="zone_id" size="&ZONE_ID_LEN;"/>
    <field name="zone_dsc" size="&ZONE_DSC_LEN;"/>
    <field name="zone_typ" size="&ZONE_TYP_LEN;"/>
    <field name="zone_set_cnt" size="&ZONE_CNT_LEN;"/>
  </struct>
  <field name="port_cnt" size="2"/>
  <struct name="oary2" multiplicity="30">
    <field name="port_id" size="&PORT_ID_LEN;"/>
    <field name="port_usage" size="1"/>
    <field name="port_stat" size="&PORT_STAT_LEN;"/>
    <field name="xfer_id" size="&TCS_NODE_LEN;"/>
  </struct>
</struct>
